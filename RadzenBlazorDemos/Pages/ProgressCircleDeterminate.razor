@using Radzen

<RadzenProgressCircle @bind-Value="@value" />
<RadzenText TextStyle="TextStyle.Subtitle2" TagName="TagName.H3">ProgressCircle with <strong>ProgressBarStyle</strong> property</RadzenText>
<RadzenStack Orientation="Orientation.Horizontal" AlignItems="AlignItems.Center" Wrap="FlexWrap.Wrap" Gap="1rem">
    <RadzenProgressCircle ProgressBarStyle="ProgressBarStyle.Primary" @bind-Value="@value" />
    <RadzenProgressCircle ProgressBarStyle="ProgressBarStyle.Secondary" @bind-Value="@value" />
    <RadzenProgressCircle ProgressBarStyle="ProgressBarStyle.Light" @bind-Value="@value" />
    <RadzenProgressCircle ProgressBarStyle="ProgressBarStyle.Dark" @bind-Value="@value" />
    <RadzenProgressCircle ProgressBarStyle="ProgressBarStyle.Success" @bind-Value="@value" />
    <RadzenProgressCircle ProgressBarStyle="ProgressBarStyle.Danger" @bind-Value="@value" />
    <RadzenProgressCircle ProgressBarStyle="ProgressBarStyle.Warning" @bind-Value="@value" />
    <RadzenProgressCircle ProgressBarStyle="ProgressBarStyle.Info" @bind-Value="@value" />
</RadzenStack>

@code {
    double value = 55;
}