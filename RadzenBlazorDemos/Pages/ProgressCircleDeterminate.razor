    <RadzenProgressCircle ProgressCircleStyle="ProgressCircleStyle.Light" @bind-Value="@value" />
    <RadzenProgressCircle ProgressCircleStyle="ProgressCircleStyle.Dark" @bind-Value="@value" />
    <RadzenProgressCircle ProgressCircleStyle="ProgressCircleStyle.Success" @bind-Value="@value" />
    <RadzenProgressCircle ProgressCircleStyle="ProgressCircleStyle.Danger" @bind-Value="@value" />
    <RadzenProgressCircle ProgressCircleStyle="ProgressCircleStyle.Warning" @bind-Value="@value" />
    <RadzenProgressCircle ProgressCircleStyle="ProgressCircleStyle.Info" @bind-Value="@value" />
</RadzenStack>

