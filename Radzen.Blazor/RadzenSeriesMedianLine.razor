@inherits RadzenSeriesConstantLine

@code {
    public RadzenSeriesMedianLine()
    {
        Stroke = "goldenrod"; // TODO: support themes
    }

    public override double Value 
    {
        get
        {
            return Series.GetMedian();
        }
        set
        {
            throw new InvalidOperationException("Value is computed and cannot be set");
        }
    }

    protected override string Name => "Median";
}
