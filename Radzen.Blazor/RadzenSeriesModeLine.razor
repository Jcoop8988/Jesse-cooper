@inherits RadzenSeriesConstantLine

@code {
    public RadzenSeriesModeLine()
    {
        Stroke = "skyblue"; // TODO: support themes
    }

    public override double Value 
    {
        get
        {
            return Series.GetMode();
        }
        set
        {
            throw new InvalidOperationException("Value is computed and cannot be set");
        }
    }

    protected override string Name => "Mode";
}
